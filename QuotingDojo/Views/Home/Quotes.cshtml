@{
    ViewData["Title"] = "Quote";
}
<link rel="stylesheet" href="~/css/quote.css">
<div>
    <a href="/">HOME</a>
    <h1>Here are the awesome quotes!</h1>

    @{
        foreach (var user in @ViewBag.users)
        {
            <p>"@user["quote"]"</p>
            <br>
            <p id="name">-@user["name"]</p>
            <p>at @user["created_at"]</p>
            <br>
            <hr>
        }
    }

</div>